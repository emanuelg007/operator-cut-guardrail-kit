{
  "name": "operator-cut-guardrail-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck",
    "prepare": "husky install",
    "probe": "playwright test -g \"@probe\"",
    "probe:trace": "playwright test -g \"@probe\" --trace on",
    "probe": "playwright test --grep @probe --reporter=list --config=playwright.config.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^9.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}
